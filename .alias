# Common aliases

alias ls='ls --color=auto --hide="*~"'
alias la="ls -aF"
alias l="ls -alhF"
alias fg=" fg"
alias ..="cd .."
alias -- -="cd -"

# Git
alias gst=" git status --untracked-files=no --ignore-submodules"
alias gstv=" git status" # "verbose"
alias gl=" git log"
alias gc="git commit" # TIL gc is an installed tool for counting graph nodes
alias gco="git checkout"
alias gd="git diff -w"
alias gdc="git diff -w --cached"
alias ga="git add"
alias gau="git add -u"
alias glg=" git log --oneline --graph --decorate"
alias gps="git push"
alias gpsh="git push"
alias gpl="git pull"
alias gpll="git pull"
alias gpq="git pull -q"
alias gfq="git fetch -q"
alias gg="git grep"

# CMU
alias smlnj="rlwrap sml"
alias msj="ssh andrew"
